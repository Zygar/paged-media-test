        "pagedjs": "^0.2.0",
        "pagedjs-cli": "^0.1.3",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-typography": "^0.16.20",
        "typography": "^0.16.21",
        "typography-theme-lincoln": "^0.16.19"
